   "source": [
    "adv = pd.read_csv('./Advertising.csv',usecols=['TV','radio','newspaper','sales'])\n",
    "adv.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "X=sm.add_constant(adv['TV'])\n",
    "y=adv['sales']\n",
    "\n",
    "linear_model=sm.OLS(y,X).fit()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
