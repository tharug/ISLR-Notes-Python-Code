    "x = np.arange(0,300,0.1)\n",
    "x = sm.add_constant(x)\n",
    "\n",
    "# Create regression line\n",
    "reg_line = linear_model.predict(x)\n",
    "\n",
    "# Predict Y values for given X values \n",
    "y_hat = linear_model.predict(X)"
